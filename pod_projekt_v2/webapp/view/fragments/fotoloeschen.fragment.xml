<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns:form="sap.ui.layout.form">
	<Dialog id="FotoLoeschDialog" >
		<customHeader>
			<Bar>
				<contentMiddle>
					<Label design="Bold" text="{i18n>fotoloeschen}"/>
				</contentMiddle>
			</Bar>
		</customHeader>
		<content>
			<form:SimpleForm >
				<form:content>
                    <Text text="{i18n>txtloeschen}"></Text>
                    <Label ></Label>
                    <Button text="{i18n>ja}" type="Accept" press="onDialogClose">
                        <layoutData>
                            <layout:GridData span="L6 M6 S6"/>
                        </layoutData>
                    </Button>
                    <Button text="{i18n>nein}" type="Reject" press="onDialogClose">
                        <layoutData>
                            <layout:GridData span="L6 M6 S6"/>
                        </layoutData>
                    </Button>
                </form:content>
			</form:SimpleForm>
		</content>
	</Dialog>
</core:FragmentDefinition>