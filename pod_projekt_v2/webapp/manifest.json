{
  "_version": "1.59.0",
  "sap.app": {
    "id": "podprojekt",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.12.4",
      "toolsId": "646bc19b-a29e-496e-a997-b80aea0e6945"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.124.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "podprojekt.i18n.i18n"
        }
      },
      "settings": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/Settings.json",
        "settings": {
          "defaultBindingMode": "TwoWay"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "TourModel": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "results": []
        }
      },
      "Kennzeichen": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "licensePlate": {}
        }
      },
      "StopModel": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "results": []
        }
      },
      "StopInformationModel": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "tour": {}
        }
      },
      "TourStartFragmentModel":{
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "tour": {}
        }
      },
      "TourStartFragmentUserModel":{
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "result": {
            "mileage":""
          }
        }
      },
      "ComplaintsModel": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "results": []
        }
      },
      "tours": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/Routes.json"
      },
      "stops": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/Stops.json"
      },
      "orders": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/Orders.json"
      },
      "loadingUnits": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "localService/mockdata/LoadingUnits.json"
      },"SpotModel": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "spot": [
            {
              "pos": "37.622882;55.755202;0",
              "tooltip": "Moscow",
              "type": "Inactive",
              "text": "12345"
      
            },
            {
              "pos": "77.1024902;28.7040592;0",
              "tooltip": "Delhi",
              "type":"Success"
            },
            {
              "pos": "-74.013327;40.705395;0",
              "tooltip": "New York",
              "type": "Error",
              "text": "1"
            },
            {
              "pos": "116.407072;39.906235;0",
              "tooltip": "Beijing",
              "type": "Warning",
              "text": "123"
            }
          ]
        }
      },
      "GeoMapProvider": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "mapConfig": {
            "MapProvider": [{
                "name": "OSM",
                "type": "",
                "description": "",
                "tileX": "256",
                "tileY": "256",
                "minLOD" : "1",
                "maxLOD": "20",
                "copyright": "Tiles Courtesy of OpenStreetMap",
                "Source": [{
                    "id": "s1",
                    "url": "https://a.tile.openstreetmap.org/{LOD}/{X}/{Y}.png"
                }]
            }],
            "MapLayerStacks": [{
                "name": "Default",
                "MapLayer": [{
                    "name": "Default",
                    "refMapProvider": "OSM",
                    "opacity": "1.0",
                    "colBkgnd": "RGB(255,255,255)"
                }]
            }]
          }
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "podprojekt.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": [
            "TargetMain"
          ]
        },
        {
          "name": "Overview",
          "pattern": "overview",
          "target": [
            "TargetOverview"
          ]
        },
        {
          "name": "ActiveTour",
          "pattern": "tour",
          "target": [
            "TargetActiveTour"
          ]
        },
        {
          "name": "StopInformation",
          "pattern": "stop",
          "target": [
            "TargetStopInformation"
          ]
        },
        {
          "name": "MapView",
          "pattern": "map",
          "target": [
            "TargetMapView"
          ]
        },
        {
          "name": "Quittierung",
          "pattern": "confirmation",
          "target": [
            "quittierung"
          ]
        },
        {
          "name": "Abladung",
          "pattern": "unloading",
          "target": [
            "AbladungTarget"
          ]
        },
        {
          "name": "Unterschrift",
          "pattern": "signature/{name}",
          "target": [
            "unterschrift"
          ]
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "main",
          "viewName": "Main"
        },
        "TargetOverview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "overviewPageId",
          "viewName": "Overview"
        },
        "TargetActiveTour": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "activetourPageId",
          "viewName": "ActiveTour"
        },
        "TargetStopInformation": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "stopinformationPageId",
          "viewName": "StopInformation"
        },
        "quittierung": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "confirmationPage",
          "viewName": "Quittierung"
        },
        "TargetMapView": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "MapViewPageId",
          "viewName": "MapView"
        },
        "AbladungTarget": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "AbladungPageId",
          "viewName": "Abladung"
        },
        "unterschrift": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "UnterschriftPageId",
          "viewName": "Unterschrift"
        }
      }
    },
    "rootView": {
      "viewName": "podprojekt.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
